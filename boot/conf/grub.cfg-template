menuentry "PXEinstall - Automated OS Installer" {
  echo "Now loading kernel and ramdisk"
  echo "Boot server: ${net_default_server}"
  echo "Local interface: ${net_default_mac}"
  echo
  echo "Please be patient ..."
  echo

  linux vmlinuz hostconf="https://${net_default_server}" client="${net_default_mac}" console=ttyS0
  initrd "initrd.img"
}